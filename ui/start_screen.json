    "$pressed_button_name": "gui.signIn",
    "$button_text": "gui.signIn"
  },

  "invite_notification_icon_button@start.custom_icon_button_base": {
    "$button_content": "start.invite_notification_icon"
  },

  "realms_pending_invite_custom_icon_button": {
    "type": "panel",
    "size": [ "100%c", "100%c" ],
