  "switch_accounts_custom_icon_button@start.custom_icon_button_base": {
    "$button_content": "start.switch_accounts_icon",
    "$pressed_button_name": "button.manage_account",
    "$button_text": "#manage_account_button_text",
    "bindings": [
      {
        "binding_name": "(not $switch_accounts_ignored)",
        "binding_name_override": "#enabled"
      }
    ]
  },

  "gathering_custom_icon_button@start.custom_icon_button_base": {
    "offset": [ 0, -2 ],
    "$button_content": "start.gathering_icon",
    "$pressed_button_name": "button.gathering",
    "$focus_override_right": "play_button",
    "bindings": [
      {
        "binding_name": "#gathering_button_enabled",
        "binding_name_override": "#enabled"
      }
    ]
  },

  "buy_game_custom_icon_button@start.custom_icon_button_base": {
    "$button_content": "start.buy_game_icon",
    "$pressed_button_name": "button.menu_buy_game",
    "$button_text": "$unlock_full_game_button_text",
    "$focus_override_right": "play_button",
    "bindings": [
      {
        "binding_name": "(not $unlock_full_game_button_ignored",
        "binding_name_override": "#enabled"
      }
    ]
  },

  "debug_custom_icon_button@start.custom_icon_button_base": {
    "$button_content": "start.gathering_icon",
    "$pressed_button_name": "button.signin",
    "$focus_override_right": "play_button"
  },

  "test_custom_icon_button@start.custom_icon_button_base": {
    "$button_content": "start.none_icon",
    "$pressed_button_name": "button.to_profile_screen"
  },

  "none_custom_icon_button@start.custom_icon_button_base": {
    "enabled": false,
    "$button_color": [ 1.0, 1.0, 1.0 ],
    "$button_content": "start.none_icon",
    "$pressed_button_name": "button.unknow",
    "$button_text": "Unknow"
  },


  //---------------------------------------------------------------------------
  // Start Screen: Sub Panel
  //---------------------------------------------------------------------------
  "title_panel": {
    "type": "panel",
    "size": [ "100%", "100%c" ],
    "controls": [
      { "title_panel_content@common_art.start_title_panel_content": { "size": [ "100%", 40 ], "offset": [ 0, 0 ], "anchor_from": "top_middle", "anchor_to": "top_middle", "$splash_text_visible": false } }
    ]
  },

  "skin_viewer_panel": {
    "type": "input_panel",
    "anchor_from": "center",
    "anchor_to": "center",
    "size": [ "80%", "80% - 16px" ],
    "offset": [ 0, 0 ],
    "gesture_tracking_button": "button.turn_doll",
    "tts_ignore_count": true,
    "button_mappings": [
      {
        "from_button_id": "button.menu_select",
        "to_button_id": "button.turn_doll",
        "mapping_type": "pressed",
        "button_up_right_of_first_refusal": true
      }
    ],
    "controls": [
      {
        "paper_doll_panel": {
          "type": "panel",
          "size": [ "100% + 16px", "100% + 6px" ],
          "controls": [
            {
              "paper_doll": {
                "type": "custom",
                "renderer": "paper_doll_renderer",
                "camera_tilt_degrees": -10,
                "starting_rotation": -20,
                "layer": 2,
                "anchor_from": "bottom_middle",
                "anchor_to": "bottom_middle",
                "rotation": "gesture_x",
                "size": [ "100%", "100%" ],
                "offset": [ 0, -14 ],
                "animation_reset_name": "screen_animation_reset",
                "anims": [
                  "@common.screen_exit_size_animation_push",
                  "@common.screen_exit_size_animation_pop",
                  "@common.screen_entrance_size_animation_push",
                  "@common.screen_entrance_size_animation_pop"
                ],
                "bindings": [
                  {
                    "binding_type": "view",
                    "source_control_name": "viewer_panel",
                    "source_property_name": "#gesture_delta_source",
                    "target_property_name": "#gesture_delta_source"
                  },
                  {
                    "binding_type": "view",
                    "source_control_name": "viewer_panel",
                    "source_property_name": "#gesture_mouse_delta_x",
                    "target_property_name": "#gesture_mouse_delta_x"
                  },
                  {
                    "binding_name": "#is_appearance_visible",
                    "binding_name_override": "#visible",
                    "binding_condition": "always"
                  }
                ]
              }
            },
            {
              "appearacne_loading_panel@common_store.progress_loading": {
                "$hide_failure_text": true,
                "$hide_progress_loading_outline": true,
                "$source_control_name": "paper_doll",
                "$source_property_name": "(not #visible)"
              }
            }
          ]
        }
      },
      {
        "paper_doll_name_tag": {
          "type": "custom",
          "renderer": "name_tag_renderer",
          "layer": 2,
          "anchor_from": "top_middle",
          "anchor_to": "bottom_middle",
          "size": [ 8, 8 ],
          "offset": [ 0, -2 ],
          "bindings": [
            {
              "binding_name": "#playername"
            }
          ]
        }
      }
    ]
  },

  "skin_panel": {
    "type": "panel",
    "anchor_to": "bottom_right",
    "anchor_from": "bottom_right",
    "size": [ "100%", "100%" ],
    "offset": [ 0, "-10% - 4px" ],
    "controls": [
      { "viewer_panel@start.skin_viewer_panel": {} }
    ]
  },


  // Top Panel
  "top_panel_image": {
    "type": "custom",
    "size": [ "100%", "100%" ],
    "renderer": "gradient_renderer",
    "gradient_direction": "horizontal",
    "color1": [ 1.0, 1.0, 1.0, 0 ],
    "color2": [ 0, 0, 0, 1.00 ],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle"
  },

  "top_tab_panel": {
    "type": "panel",
    "size": [ "200%", 32 ],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "controls": [
      { "background_image@top_panel_image": { "size": [ "45%", "100%" ], "anchor_from": "right_middle", "anchor_to": "right_middle" } },
      {
        "game_text_panel": {
          "type": "panel",
          "size": [ "50% - 10px", "100% - 4px" ],
          "anchor_from": "center",
          "anchor_to": "center",
          "controls": [
            { "gamertag@start.gamertag": {} },
            { "game_version@start.game_version": {} }
          ]
        }
      }
    ]
  },

  "game_copyright": {
    "type": "stack_panel",
    "size": [ "default", "100%" ],
    "offset": [ 0, "27%" ],
    "anchor_from": "left_middle",
    "anchor_to": "left_middle",
    "controls": [
      {
        "copyright": {
          "type": "label",
          "color": "$main_header_text_color",
          "layer": 2,
          "text": "menu.copyright",
          "size": [ "default", 10 ],
          "max_size": [ "100%", "100%" ],
          "text_alignment": "left"
        }
      }
    ]
  },

  "game_version": {
    "type": "stack_panel",
    "size": [ "100%", "100%c" ],
    "controls": [
      {
        "development_version": {
          "type": "label",
          "size": [ "100%", "default" ],
          "max_size": [ "100%", "default" ],
          "layer": 2,
          "text": "#development_version",
          "text_alignment": "right",
          "color": "$main_header_text_color",
          "bindings": [
            { "binding_name": "#development_version" }
          ]
        }
      },
      {
        "version": {
          "type": "label",
          "size": [ "100%", "default" ],
          "max_size": [ "100%", "default" ],
          "layer": 2,
          "text": "#version",
          "text_alignment": "right",
          "color": "$main_header_text_color",
          "bindings": [
            { "binding_name": "#version" }
          ]
        }
      }
    ]
  },

  "gamertag": {
    "type": "panel",
    "size": [ "100%c", "100%" ],
    "anchor_from": "left_middle",
    "anchor_to": "left_middle",
    "controls": [
      { "gamertag@gamertag_panel": { } }
    ]
  },


  // Gamertag
  "gamertag_label": {
    "type": "label",
    "color": "$gamertag_color",
    "layer": 1,
    "text": "#gamertag_label",
    "locked_alpha": 0.5,
    "size": [ "default", "100%" ],
    "offset": "$gamertag_offset",
    "font_type": "MinecraftTen",
    "font_scale_factor": 1.3,
    "text_alignment": "center",
    "bindings": [
      { "binding_name": "#gamertag_label" }
    ]
  },

  "gamertag_panel": {
    "type": "panel",
    "size": [ "100%c", "30px" ],
    "offset": [ 0, 0 ],
    "anchor_from": "center",
    "anchor_to": "center",
    "controls": [
      {
        "gamertag_pic_and_label": {
          "type": "stack_panel",
          "anchor_from": "center",
          "anchor_to": "center",
          "orientation": "horizontal",
          "size": [ "100%c", 26 ],
          "offset": [ -9, 0 ],
          "controls": [
            {
              "gamerpic_with_border": {
                "type": "panel",
                "size": [ "100%c", "100%" ],
                "controls": [
                  {
                    "gamerpic": {
                      "type": "image",
                      "size": [ 26, 26 ],
                      "layer": 1,
                      "bindings": [
                        {
