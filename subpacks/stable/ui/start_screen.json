  //---------------------------------------------------------------------------
  // Start Screen: Sub Panel
  //---------------------------------------------------------------------------
  "void_panel": {
    "type": "panel",
    "size": [ 10, "100%" ]
  },

  "title_panel": {
    "type": "stack_panel",
    "size": [ "100%", "100%" ],
    "orientation": "vertical",
    "$title_offset_padding|default": [ "100%", "30%" ],
    "variables": [
      {
        "requires": "($pocket_screen)",
        "$title_offset_padding": [ "100%", "40%" ]
      }
    ],
    "controls": [
      { "title_offset_padding": { "type": "panel", "size": "$title_offset_padding" } },
      { "title_panel_content@common_art.start_title_panel_content": {} }
    ]
  },

  "skin_viewer_panel": {
    "type": "input_panel",
    "anchor_from": "center",
    "anchor_to": "center",
    "size": [ "50%", "50% - 16px" ],
    "offset": [ 0, 0 ],
    "gesture_tracking_button": "button.turn_doll",
    "tts_ignore_count": true,
    "button_mappings": [
      {
        "from_button_id": "button.menu_select",
        "to_button_id": "button.turn_doll",
        "mapping_type": "pressed",
        "button_up_right_of_first_refusal": true
      }
    ],
    "controls": [
      {
        "paper_doll_panel": {
          "type": "panel",
          "size": [ "100% + 16px", "100% + 6px" ],

          "controls": [
            {
              "paper_doll": {
                "type": "custom",
                "renderer": "paper_doll_renderer",
                "camera_tilt_degrees": -10,
                "starting_rotation": 30,
                "layer": 2,
                "anchor_from": "bottom_middle",
                "anchor_to": "bottom_middle",
                "rotation": "gesture_x",
                "size": [ "100%", "100%" ],
                "offset": [ 0, -22 ],
                "animation_reset_name": "screen_animation_reset",
                "anims": [
                  "@common.screen_exit_size_animation_push",
                  "@common.screen_exit_size_animation_pop",
                  "@common.screen_entrance_size_animation_push",
                  "@common.screen_entrance_size_animation_pop"
                ],
                "bindings": [
                  {
                    "binding_type": "view",
                    "source_control_name": "viewer_panel",
                    "source_property_name": "#gesture_delta_source",
                    "target_property_name": "#gesture_delta_source"
                  },
                  {
                    "binding_type": "view",
                    "source_control_name": "viewer_panel",
                    "source_property_name": "#gesture_mouse_delta_x",
                    "target_property_name": "#gesture_mouse_delta_x"
                  },
                  {
                    "binding_name": "#is_appearance_visible",
                    "binding_name_override": "#visible",
                    "binding_condition": "always"
                  }
                ]
              }
            },
            {
              "appearacne_loading_panel@common_store.progress_loading": {
                "$hide_failure_text": true,
                "$hide_progress_loading_outline": true,
                "$source_control_name": "paper_doll",
                "$source_property_name": "(not #visible)"
              }
            }
          ]
        }
      },
      {
        "paper_doll_name_tag": {
          "type": "custom",
          "renderer": "name_tag_renderer",
          "layer": 2,
          "anchor_from": "top_middle",
          "anchor_to": "bottom_middle",
          "size": [ 8, 8 ],
          "offset": [ 0, 0 ],
          "bindings": [
            {
              "binding_name": "#playername"
            }
          ]
        }
      }
    ]
  },

  "skin_panel": {
    "type": "panel",
    "anchor_to": "bottom_right",
    "anchor_from": "bottom_right",
    "size": [ "100%", "100%" ],
    "offset": [ 0, "-10% - 4px" ],
    "controls": [
      { "viewer_panel@start.skin_viewer_panel": {} }
    ]
  },


  "test_panel": {
    "type": "custom",
    "renderer": "web_view_renderer",
    "property_bag": {
      "#web_link": "https://www.naver.com"
    },
    "size": [ "100%", "100%" ],
    "anchor_from": "center",
    "anchor_to": "center"
  },


  //Top Panel
  "top_panel_image": {
    "type": "image",
    "size": [ "100%", "100%" ],
    "texture": "textures/ui/ScreenTopBar",
    "alpha": 0.6,
    "anchor_from": "top_middle",
    "anchor_to": "top_middle"
  },

  "top_bar_filler": {
    "type": "image",
    "texture": "textures/ui/ScreenTopBarFiller"
  },

  "top_text_panel@top_panel_image": {
    "size": [ "200%", 22 ],
    "controls": [
      {
        "game_text_panel": {
          "type": "panel",
          "size": [ "50% - 10px", "100%" ],
          "controls": [
            { "game_copyright@start.game_copyright": {} },
            { "game_version@start.game_version": {} }
          ]
        }
      }
    ]
  },

  "game_copyright": {
    "type": "stack_panel",
    "size": [ "default", "100%" ],
    "offset": [ 0, "27%" ],
    "anchor_from": "left_middle",
    "anchor_to": "left_middle",
    "controls": [
      {
        "copyright": {
          "type": "label",
          "color": "$main_header_text_color",
          "layer": 2,
          "text": "menu.copyright",
          "size": [ "default", 10 ],
          "max_size": [ "100%", "100%" ],
          "text_alignment": "left"
        }
      }
    ]
  },

  "game_version": {
    "type": "stack_panel",
    "size": [ "default", "100%" ],
    "offset": [ 0, 1.5 ],
    "anchor_from": "right_middle",
    "anchor_to": "right_middle",
    "controls": [
      {
        "development_version": {
          "type": "label",
          "size": [ "100%", 10 ],
          "max_size": [ "100%", "100%" ],
          "layer": 2,
          "text": "#development_version",
          "text_alignment": "right",
          "color": "$main_header_text_color",
          "bindings": [
            { "binding_name": "#development_version" }
          ]
        }
      },
      {
        "version": {
          "type": "label",
          "size": [ "100%", 10 ],
          "max_size": [ "100%", "100%" ],
          "layer": 2,
          "text": "#version",
          "text_alignment": "right",
          "color": "$main_header_text_color",
          "bindings": [
            { "binding_name": "#version" }
          ]
        }
      }
    ]
  },


  "gamertag_label": {
    "type": "label",
    "color": "$gamertag_color",
    "layer": 1,
    "text": "#gamertag_label",
    "locked_alpha": 0.5,
    "size": [ "default", "100%" ],
    "offset": "$gamertag_offset",
    "font_type": "MinecraftTen",
    "font_scale_factor": 1.3,
    "text_alignment": "center",
    "bindings": [
      { "binding_name": "#gamertag_label" }
    ]
  },

  "gamertag_panel": {
    "type": "panel",
    "size": [ "100%c", "22px" ],
    "offset": [ 3, -4 ],
    "anchor_from": "center",
    "anchor_to": "center",
    "controls": [
      {
        "gamertag_pic_and_label": {
          "type": "stack_panel",
          "anchor_from": "center",
          "anchor_to": "center",
          "orientation": "horizontal",
          "size": [ "100%c", 22 ],
          "offset": [ -14, 0 ],
          "controls": [
            {
              "gamerpic_with_border": {
                "type": "panel",
                "size": [ "100%c", "100%" ],
                "controls": [
                  {
                    "gamerpic": {
                      "type": "image",
                      "size": [ 22, 22 ],
                      "layer": 1,
                      "bindings": [
                        {
                          "binding_name": "#gamerpic_texture_path",
                          "binding_name_override": "#texture"
                        },
                        {
                          "binding_name": "#gamerpic_texture_location_type",
                          "binding_name_override": "#texture_file_system"
                        }
                      ]
                    }
                  },
                  {
                    "border_white@common.focus_border_white": {
                      "layer": 3,
                      "size": [ 22, 22 ]
                    }
                  }
                ]
              }
            },
            { "spacer": { "type": "panel", "size": [ 2, 0 ] } },
            {
              "gamertag_label_panel": {
                "type": "panel",
                "size": [ "100%c - 75%c", "100%" ],
                "$gamertag_color|default": [ 1.0, 1.0, 1.0 ],
                "controls": [
                  {
                    "gamertag_label_pink@start.gamertag_label": {
                      "$gamertag_color": [ 1.0, 0.0, 1.0 ],
                      "$gamertag_offset": [ 1, 3 ]
                    }
                  },
                  {
                    "gamertag_label_lightBlue@start.gamertag_label": {
                      "$gamertag_color": [ 0.0, 1.0, 1.0 ],
                      "$gamertag_offset": [ -1, 2 ]
